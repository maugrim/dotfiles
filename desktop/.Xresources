Xft.autohint: 0
Xft.lcdfilter: lcddefault
Xft.hintstyle: hintslight
Xft.hinting: 1
Xft.rgba: rgb

Emacs*.geometry: 120x40
Emacs*.background: #3F3F3F
Emacs*.pane.menubar.shadowThickness: 1
Emacs*.pane.menubar.margin: 1
Emacs*.toolBar: off
Emacs*.menuBar: off
Emacs*.verticalScrollBars: off
Emacs*.cursorColor: white

URxvt*.geometry: 120x40
URxvt*.depth: 32
URxvt*.foreground: #FFFFFF
URxvt*.background: [90]#000000
URxvt*.cursorColor: white
URxvt*.colorUL: #c5f779
URxvt*.underlineColor: #c5f779
URxvt*.font: xft:Consolas:size=12
URxvt*.iconFile: /usr/share/icons/Humanity/apps/64/utilities-terminal.svg
URxvt*.letterSpace: -2
URxvt*.scrollBar: false
URxvt*.saveLines: 10000
URxvt*.termName: xterm-256color
URxvt*.transparent: true
URxvt*.shading: 20

URxvt.perl-ext-common: default,matcher
URxvt.url-launcher: /usr/bin/xdg-open
URxvt.matcher.button: 1
URxvt.matcher.rend.0: Uline Bold fg4

*foreground: #FFFFFF
*background: #000010

!black
*color0:     #252525
*color8:     #454545
!red
*color1:     #EF6769
*color9:     #FC7CA5
!green
*color2:     #A6E22E
*color10:    #B6E354
!yellow
*color3:     #FD971F
*color11:    #FD971F
!blue
*color4:     #6495ED
*color12:    #87CEEB
!magenta
*color5:     #DEB887
*color13:    #996600
!cyan
*color6:     #B0C4DE
*color14:    #87CEEB
!white
*color7:     #DBDCDC
*color15:    #FDFDFD