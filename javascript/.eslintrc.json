{
  "extends": "eslint:recommended",
  "rules": {
    "quotes": 0,
    "no-console": 0,
    "linebreak-style": ["error", "unix"],
    "semi": ["error", "always"]
  },
  "env": {
    "es6": true,
    "node": true,
    "browser": true
  },
  "parserOptions": {
    "sourceType": "module",
    "ecmaVersion": 8
  }
}
